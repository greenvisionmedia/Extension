{
  "manifest_version": 3,
  "name": "Pusherman",
  "description": "Aint I clean bad machine super cool super mean",
  "version": "0",
  "icons": {
    "16": "/icons/icon16.png",
    "32": "/icons/icon32.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://webflow.com/design/*"],
      "js": ["public/content.min.js"],
      "css": ["public/stylesheet.min.css"]
    }
  ],
  "background": {
    "service_worker": "public/background.min.js"
  },
  "permissions": [
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://webflow.com/design/*"
  ]
}